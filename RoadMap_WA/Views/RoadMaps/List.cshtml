<h1 align="center" style="margin-bottom: 40px;">Все RoadMap-ы</h1>

<table width="100%" cellspacing="0" cellpadding="5">
	<tr>
		@foreach (var RMap in Model.AllRoadMaps)
		{
			<td width="200" valign="top">
				<div>
					<h2>Содержание: @RMap.Data</h2>
					<h3>Дети: </h3>
					@{
						foreach (var child in RMap.Children)
						{
							<p>@child.Data</p>
						}
					}
				</div>
			</td>
		}
	</tr>
</table>